/* pages/add/add.wxss */
.release{
  width: 100%;
  height: 100%;
  background: #eee;
  position:fixed;
}
.from-jl{
  background:#33CCFF;
  width:90%;
  height:200rpx;
  margin:100rpx auto 0;
  border-radius:20rpx;
  display:flex;
  flex-direction:column;
  justify-content: center;
  align-items: center;
}
.title{
  color: white;
  font-size: 30rpx;
  display: block;
  letter-spacing:5rpx;
}
.describe{
  color: white;
  font-size: 20rpx;
  display: block;
  margin:10rpx 0;
}
.btn{
  color: #33CCFF;
  display: block;
  font-size: 20rpx;
  width: 300rpx;
  height: 40rpx;
  background: white;
  text-align:center;
  line-height:40rpx;
  border-radius:20rpx;
}