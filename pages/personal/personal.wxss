/* pages/personal/personal.wxss */
page{
  width: 100%;
  height: 100%;
  background-color: #EEEEEE;  /*灰色背景*/ 
  font:30rpx "微软雅黑";
}
/*模板start  */
.section{
  width: 100%;
  background-color: white;
  margin-top: 20rpx;
}
.getTemp{
  border-top:1rpx solid gainsboro;
  height: 100rpx;
}
.mineMarginTop{
  margin-top: 0;
}
.mineNavTo{
  height: 100%;
  width: 100%;
  display: flex;
  align-items: center;
}
.tempIcon{
  height: 32rpx;
  width: 32rpx;
  margin-left: 30rpx;
}
.tempName{
  width: calc(100% - 330rpx);
  margin: 0 10rpx;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}
.tempRlCont{
  width: 200rpx;
  text-align: right;
  padding-right: 10rpx;
}
.rightShotImg{
  width: 12rpx; 
  height: 24rpx;
}
/*模板end  */